.container {
  display: flex;
  flex-direction: column;
  padding: 20px;
}

.search-bar {
  width: 100%;
  height: 40px;
  margin-bottom: 20px;
  padding: 10px;
  border: 1px solid #ccc;
  border-radius: 5px;
}

.tabs {
  display: flex;
  justify-content: space-around;
  margin-bottom: 20px;
}

.tab-button {
  width: 100px;
  height: 40px;
}

.application-list {
  display: flex;
  flex-direction: column;
}

.application-item {
  display: flex;
  align-items: center;
  margin-bottom: 20px;
}

.session-image {
  width: 100px;
  height: 100px;
  margin-right: 20px;
}

.application-details {
  display: flex;
  flex-direction: column;
}

.title {
  font-size: 18px;
  margin-bottom: 10px;
}

.time {
  font-size: 16px;
  margin-bottom: 10px;
}

.status {
  font-size: 16px;
  color: green;
}
